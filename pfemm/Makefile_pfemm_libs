clean_fsolver_fsolver: 
	$(MAKE) -C fsolver clean -f fsolver.cbp.mak

clean_fmesher_fmesher: 
	$(MAKE) -C fmesher clean -f fmesher.cbp.mak

clean_liblua_liblua: 
	$(MAKE) -C liblua clean -f liblua.cbp.mak

clean_fpproc_fpproc: 
	$(MAKE) -C fpproc clean -f FPProc.cbp.mak

